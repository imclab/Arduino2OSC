//for control: 0/0

function(outletNum, routeNum) {
	  routeNum = 0; 
	  var fx = new sensorCTL(outletNum, routeNum);

      fx.colName = 0;
      var dropdownState = [0,0,"composition","video/effect2","param1/values ${0}","null"]
;

      var sensorUIState = [[0,0,0.8999999761581421]
,[0,1,0.9900000095367432]
,["0",2,0]
,[0,3,0.9900000095367432]
,[0,4,0.12999999523162842]
,[0,5,0.9900000095367432]
,[0,6,1]
,[0,7,1]
,["0",8,1]
,["0",9,0]
]
;

      dropdownState[1] = routeNum; //replace old routeNum with current when loading
      refreshDropdownGroup(dropdownState);

      for (var j in sensorUIState) {
      		debugPrint('\n\nrefreshing' + j);refreshSensorUIGroup(sensorUIState[j]);
}
      return fx;
}

